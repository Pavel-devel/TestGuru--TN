<h1>All tests</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.title %></td>
        <td><%= link_to "Show", test_questions_path(test) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
